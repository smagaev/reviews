{
    "name": "nicolask/slimmer-pdo",
    "description": "PDO database library for Slim Framework",
    "version": "1.11.0",
    "type": "library",
    "keywords": ["pdo", "database", "slim", "framework"],
    "homepage": "https://github.com/nicolask/Slim-PDO",
    "license": "MIT",
    "authors": [
        {
            "name": "Fabian de Laender",
            "email": "fabian@faapz.nl",
            "homepage": "http://faapz.nl",
            "role": "Developer"
        },
        {
            "name": "Nicolas Krueger",
            "email": "nicolas.krueger@gmail.com",
            "role": "Maintainer"
        }
    ],
    "support": {
        "issues": "https://github.com/nicolask/Slim-PDO/issues",
        "docs": "https://github.com/nicolask/Slim-PDO/blob/master/docs/README.md"
    },
    "conflict": {
        "slim/pdo": "*"
    },
    "require": {
        "php": "^7.1",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Slim\\PDO\\": "src/PDO/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slim\\PDO\\": "tests/PDO/"
        }
    }
}
